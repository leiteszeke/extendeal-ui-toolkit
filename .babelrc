{
	"presets": [
		"@babel/react",
		"@babel/preset-env",
		"@babel/typescript"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread",
		["module-resolver", {
      "root": ["./src"],
      "alias": {
        "#components": "./src/components",
        "#colors": "./src/components/Colors"
      }
    }]
	]
}
