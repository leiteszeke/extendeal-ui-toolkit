    "eslint-plugin-react": "^7.21.3",
    "eslint-watch": "^7.0.0",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-svg-loader": "^3.0.3",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
