{
  "name": "@leiteszeke/ui-toolkit",
  "version": "0.0.3",
  "description": "UI Toolkit",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build:dev": "webpack",
    "build:prod": "webpack -p",
    "watch": "webpack --watch --watch-poll ",
    "lint": "eslint './src/**/*.tsx'",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "prettier": "prettier --check ./src/**/*.{js,jsx,ts,tsx}",
    "prettier:fix": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
    "storybook": "start-storybook -p 6006",
    "deploy": "cp vercel.json storybook-static && vercel --prod",
    "build:storybook": "build-storybook",
    "commit": "npx git-cz"
  },
  "homepage": "https://ui-toolkit.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/leiteszeke/ui-toolkit.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache",
    "snapshotSerializers": []
  },
  "keywords": [
    "ui",
    "toolkit",
    "react"
  ],
  "author": "",
  "contributors": [
    {
      "name": "Ezequiel Leites",
      "email": "ezequiel@leites.dev"
    },
    {
      "name": "Manuel Uso",
      "email": "musoxvi@gmail.com"
    },
    {
      "name": "Nicolas Villalonga",
      "email": "nicolas.villalonga@extendeal.com"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@extendeal/ext-commons": "^0.1.8",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.51",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.3.0",
