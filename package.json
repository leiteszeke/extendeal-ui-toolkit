{
  "name": "@leiteszeke/ui-toolkit",
  "version": "0.0.1",
  "description": "UI Toolkit",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build:dev": "webpack",
    "build:prod": "webpack -p",
    "watch": "webpack --watch --watch-poll ",
    "lint": "eslint './src/**/*.tsx'",
    "lint:fix": "eslint './src/**/*.{js,jsx,ts,tsx}' --fix",
    "prettier": "prettier --check ./src/**/*.{js,jsx,ts,tsx}",
    "prettier:fix": "prettier --write ./src/**/*.{js,jsx,ts,tsx}",
    "storybook": "start-storybook -p 6006",
    "deploy": "cp vercel.json storybook-static && vercel --prod",
    "build:storybook": "build-storybook"
  },
  "homepage": "https://ui-toolkit.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/leiteszeke/ui-toolkit.git"
  },
  "storybook-deployer": {
    "gitUsername": "leiteszeke",
    "gitEmail": "ezequiel@leites.dev",
    "commitMessage": "Deploy Storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache",
    "snapshotSerializers": []
  },
  "keywords": [
    "ui",
    "toolkit",
    "react"
  ],
  "author": "",
  "contributors": [
    {
      "name": "Ezequiel Leites",
      "email": "ezequiel@leites.dev"
    },
    {
      "name": "Manuel Uso",
      "email": "musoxvi@gmail.com"
    },
    {
      "name": "Nicolas Villalonga",
      "email": "nicolas.villalonga@extendeal.com"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@extendeal/ext-commons": "^0.1.8",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@storybook/storybook-deployer": "^2.8.6",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/numeral": "0.0.25",
    "@types/react": "^16.7.6",
    "@types/react-datepicker": "^2.8.1",
    "@types/react-dom": "^16.0.9",
    "@types/styled-components": "^4.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-watch": "^6.0.0",
    "gh-pages": "^2.1.1",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-svg-loader": "^3.0.3",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "vanilla-typescript": "^0.5.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "core-js": "3",
    "moment": "^2.24.0",
    "react-animate-height": "^2.0.15",
    "react-datepicker": "^2.9.6",
    "react-live": "^2.2.0",
    "styled-components": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
